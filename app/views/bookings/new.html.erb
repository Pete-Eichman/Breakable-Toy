<%= form_for [@parking_pass, @booking] do |f| %>
  <div class="row center-align" id="edit-page">
    </br>
    <div class="col s12 m10 offset-m1 l8 push-l1" id = "add_pass">
      <div>
        <%= f.label :first_name, class: "new_booking_label" %>
        <%= f.text_field :first_name, class: "text_area" %>
      </div>

      <div>
        <%= f.label :last_name, class: "new_booking_label" %>
        <%= f.text_field :last_name, class: "text_area" %>
      </div>
      <div>
        <%= f.label :phone_number, class: "new_booking_label" %>
        <%= f.text_field :phone_number, class: "text_area" %>
      </div>

      <div>
        <%= f.label :start_time, class: "new_booking_label" %>
        <%= f.text_field :start_time %>
      </div>

      <div>
        <%= f.label :end_time, class: "new_booking_label" %>
        <%= f.text_field :end_time, class: "text_area" %>
      </div>

      <div>
        <%= f.label :date, class: "new_booking_label" %>
        <%= f.text_field :date, class: "text_area" %>
      </div>
    </div>
    <div>
      <%= f.submit "Create Booking", class: "btn" %>
    </div>
  </div>
<% end %>
