<%= form_for([@parking_pass, @booking]) do |f| %>
  <div class="row">
    <div class="col s8">
      <%= f.label :start_time, class: "new_booking_label" %>
      <%= f.text_field :start_time %>
    </div>
  </div>

  <div class="row">
    <div class="col s8">
      <%= f.label :end_time, class: "new_booking_label" %>
      <%= f.text_field :end_time, class: "text_area" %>
    </div>
  </div>

  <div class="row">
    <div class="col s8">
      <%= f.submit "Create Booking", class: "btn" %>
    </div>
  </div>
<% end %>
